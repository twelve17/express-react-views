    "format": "prettier --write *.js *.mjs package.json .eslintrc.js 'examples/**/*.{js,jsx,json}' 'test/*.{js,jsx,json}'",
    "test": "node test/index.mjs",
    "test-on-travis": "yarn run test && yarn run lint"
  },
  "author": "Paul Oâ€™Shannessy <paul@oshannessy.com>",
